@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AdminHeaderPartial")

<body class="hold-transition sidebar-mini layout-fixed">

    <div class="wrapper">

        @await Component.InvokeAsync("_AdminNavbarPartial")

        @await Component.InvokeAsync("_AdminSidebarPartial")


        <div class="main-panel ">

            @RenderBody()

        </div>
       
    </div>

    @await Component.InvokeAsync("_AdminFooterPartial")

    @await Component.InvokeAsync("_AdminScriptPartial")

</body>
</html>
